{
  "name": "@uttori/plugin-upload-multer",
  "version": "2.1.0",
  "description": "Uttori upload provider using Multer.",
  "author": "Matthew Callis <matthew.callis@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uttori-plugin-upload-multer.git"
  },
  "bugs": {
    "url": "https://github.com/uttori-plugin-upload-multer/issues"
  },
  "homepage": "https://github.com/uttori/uttori-plugin-upload-multer",
  "keywords": [
    "upload",
    "multer"
  ],
  "engines": {
    "node": ">= 16"
  },
  "peerDependencies": {
    "express": "^4",
    "multer": "^1.4.2"
  },
  "optionalDependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "ava": "^4.2.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-anti-trojan-source": "^1.1.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-xss": "^0.1.11",
    "jsdoc": "^3.6.10",
    "jsdoc-to-markdown": "^7.1.1",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "release-it": "^15.0.0",
    "sinon": "^14.0.0",
    "supertest": "^6.2.3",
    "typescript": "^4.7.2"
  },
  "files": [
    "src/*",
    "types/*"
  ],
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./src/index.js",
      "import": "./esm/index.js"
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "ignoredByWatcher": [
      "!src/**/*.js"
    ],
    "concurrency": 1,
    "failFast": false,
    "tap": false,
    "verbose": true
  },
  "nyc": {
    "cache": true,
    "all": true,
    "check-coverage": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": []
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "scripts": {
    "docs": "jsdoc2md --private --example-lang js --template rm.hbs src/index.js > README.md; jsdoc2md --private --example-lang js src/index.js > docs/upload-multer.md",
    "docs-serve": "docsify init ./docs && docsify serve ./docs",
    "lint": "eslint .",
    "make-types": "rm -fr types/ && mkdir types && tsc --project .tsconfig.json",
    "release": "release-it",
    "report": "nyc report --reporter=html",
    "test": "NODE_ENV=test nyc ava",
    "test-debug": "DEBUG=Uttori* NODE_ENV=test nyc ava --serial --concurrency=1",
    "validate": "npm ls"
  },
  "directories": {
    "test": "test"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
